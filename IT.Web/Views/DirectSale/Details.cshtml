@model List<IT.Core.ViewModels.DirectSaleViewModel>
@{
    ViewBag.Title = "Aw Fuel - Direct Sale Details";

    List<IT.Core.ViewModels.DirectSaleViewModel> directSaleViewModels = new List<IT.Core.ViewModels.DirectSaleViewModel>();
    directSaleViewModels = Model as List<IT.Core.ViewModels.DirectSaleViewModel>;
}



<div class="row">
    <div class="col-xl-12">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    <a class="btn btn-md btn-primary" href="/DirectSale/DirectsaleOrderAdd"> <i class="fal fa-plus-circle"></i>  Add New <span class="fw-300"><i>Direct Sale Order</i></span></a>
                </h2>

            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <!--      <div class="panel-tag">
                                <p>Drivers Base Information</p>


                            </div>-->
                    <!-- datatable start -->
                    <table id="dt-basic-example" class="table table-bordered table-hover table-striped w-100">
                        <thead class="bg-primary-600">
                            <tr>
                                <th>Comany Name</th>
                                <th>Quantity</th>
                                <th>Traffic Plate Number</th>
                                <th>User Name</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>

                            @foreach (var item in directSaleViewModels)
                            {

                                <tr>
                                    <td>@item.CompanyName</td>
                                    <td>@item.Quantity</td>
                                    <td>@item.TraficPlateNumber</td>
                                    <td>@item.UserName</td>
                                    <td>
                                        <a href="/DirectSale/Details/@item.VehicleId"><i class="fal fa-eye"></i></a> &nbsp; &nbsp;
                                        <i class="fal fa-edit"></i>&nbsp; &nbsp;
                                       
                                    </td>

                                </tr>
                            }

                        </tbody>

                    </table>
                    <!-- datatable end -->
                </div>
            </div>
        </div>
    </div>
</div>



