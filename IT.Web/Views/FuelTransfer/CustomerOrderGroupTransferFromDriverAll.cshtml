@model List<IT.Core.ViewModels.TransferFromDriverViewModel>
@{
    ViewBag.Title = "CustomerOrderGroupTransferFromDriverAll";
    Layout = "~/Views/Shared/_layoutAdmin.cshtml";

    List<IT.Core.ViewModels.TransferFromDriverViewModel> transferFromDriverViewModels = new List<IT.Core.ViewModels.TransferFromDriverViewModel>();
    transferFromDriverViewModels = Model as List<IT.Core.ViewModels.TransferFromDriverViewModel>;
}


<div class="row">
    <div class="col-xl-12">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    <a href="#">
                        <i class="fal fa-plus-circle"></i>  Customer Order Group <span class="fw-300"><i>Transfer</i></span>

                    </a>
                </h2>

            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <!--      <div class="panel-tag">
                                <p>Drivers Base Information</p>


                            </div>-->
                    <!-- datatable start -->
                    <table id="dt-basic-example" class="table table-bordered table-hover table-striped w-100">
                        <thead class="bg-primary-600">
                            <tr>
                                <th>Created Date</th>
                                <th>From Driver</th>
                                <th>To Driver</th>
                                <th>Is Full Or Partial</th>
                                <th>Quanity</th>
                                <th>Description</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in transferFromDriverViewModels)
                            {

                                <tr>
                                    <td>@item.CreatedDate</td>
                                    <td>@item.FromDriver</td>
                                    <td>@item.ToDriver</td>
                                    <td>
                                        @if (item.IsFullOrder == true)
                                        {
                                            <p style="color:green">@item.IsFullOrder</p>
                                        }
                                        else
                                        {
                                            <p style="color:red">@item.IsFullOrder</p>
                                        }
                                    </td>
                                    <td>@item.Qunantity</td>
                                    <td>@item.Descriptions</td>
                                    <td>
                                        <a href="/Driver/Details/@item.Id"><i class="fal fa-eye"></i> </a> &nbsp; &nbsp;
                                        <a href="/Driver/Edit/@item.Id"><i class="fal fa-edit"></i></a>&nbsp; &nbsp;
                                        <i class="fal fa-trash-alt"></i>
                                    </td>
                                </tr>
                            }

                        </tbody>

                    </table>
                    <!-- datatable end -->
                </div>
            </div>
        </div>
    </div>
</div>


@section scripts{


        <script>
        $(document).ready(function () {
            $("#MasterMenu").attr("aria-expanded", "true");
            $('#MasterUl').css('display', 'block');
            $('#FuelTransferLi').addClass('active');
        });

        </script>

}