@{
    /**/

    ViewBag.Title = "Customer dashboard";


    List<IT.Core.ViewModels.CustomerNotificationViewModel> customerNotificationViewModels = new List<IT.Core.ViewModels.CustomerNotificationViewModel>();
    customerNotificationViewModels = ViewBag.customerNotificationViewModels as List<IT.Core.ViewModels.CustomerNotificationViewModel>;

    IT.Core.ViewModels.CustomerOrderStatistics customerOrderStatistics = new IT.Core.ViewModels.CustomerOrderStatistics();
    customerOrderStatistics = ViewBag.customerOrderStatistics as IT.Core.ViewModels.CustomerOrderStatistics;

    IT.Core.ViewModels.FuelPricesViewModel fuelPricesViewModel = new IT.Core.ViewModels.FuelPricesViewModel();
    fuelPricesViewModel = ViewBag.fuelPricesViewModel as IT.Core.ViewModels.FuelPricesViewModel;

    List<IT.Core.ViewModels.FuelPricesViewModel> fuelPricesViewModels = new List<IT.Core.ViewModels.FuelPricesViewModel>();
    fuelPricesViewModels = ViewBag.fuelPricesViewModels as List<IT.Core.ViewModels.FuelPricesViewModel>;
}

<style>
    .menu4 .panel-toolbar a {
        display: none;
    }
</style>

<div class="row">
