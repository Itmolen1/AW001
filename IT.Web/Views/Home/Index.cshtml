    @section Scripts{

        <script>
            $(document).ready(function () {
                $('.carousel').carousel();

                //$('#MyBookinMenu').attr('aria-expanded', true);
                $("#btn1").hide();
                $("#btn2").click(function () {
                    $("#btn1").show();
                    $("#btn2").hide();
                });

                $('#btnNewBookin').click(function () {
                    $("#CustomerMyBookinMenu").attr("aria-expanded", "true");
                    $('#CustomerbookingUl').css('display', 'block');
                });

            })
        </script>
        <script src="~/Scripts/CustomScript/TokenScript.js"></script>

