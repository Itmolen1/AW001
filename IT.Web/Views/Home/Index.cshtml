                    @customerOrderStatistics.TotalDrivers
                    <small class="m-0 l-h-n">Driver</small>
                </h3>
            </div>
            <a style="color:white" href="/Driver/Index">
                <i class="fal fa-list  position-absolute pos-right pos-top" style="font-size:2.5rem; margin-top:30px; margin-right:15px"></i>
            </a>
        </div>
    </div>
    <div class="col-sm-6 col-xl-6">
        <div class="p-3 bg-primary-600 rounded overflow-hidden position-relative text-white mb-g">
            <div class="">
                <h3 class="display-4 d-block l-h-n m-0 fw-500">
                    @customerOrderStatistics.TotalVehicles
                    <small class="m-0 l-h-n">Vehicle</small>
                </h3>
            </div>
            <a style="color:white" href="/Vehicle/Index">
                <i class="fal fa-list  position-absolute pos-right pos-top" style="font-size:2.5rem; margin-top:30px; margin-right:15px"></i>
            </a>
        </div>
    </div>
    <div class="col-sm-6 col-xl-6">
        <div class="p-3 bg-primary-600 rounded overflow-hidden position-relative text-white mb-g">
            <div class="">
                <h3 class="display-4 d-block l-h-n m-0 fw-500">
                    @customerOrderStatistics.TotolRequestedQuantity
                    <small class="m-0 l-h-n">Today's Accepted Requested & Sent Quantity</small>
                </h3>
            </div>
            <i class="fal fa-map-marker position-absolute pos-right pos-top opacity-90 " style="font-size: 3rem; margin-top:30px; margin-right:15px"></i>
        </div>
    </div>
    <div class="col-sm-6 col-xl-6">
        <div class="p-3 bg-primary-600 rounded overflow-hidden position-relative text-white mb-g">
            <div class="">
                <h3 class="display-4 d-block l-h-n m-0 fw-500">
                    @customerOrderStatistics.TotalDeliverdQuantity
                    <small class="m-0 l-h-n">Today's Deliverd Quantity</small>
                </h3>
            </div>
            <i class="fal fa-location-arrow position-absolute pos-right pos-top opacity-90 " style="font-size: 3rem; margin-top:30px; margin-right:15px"></i>
        </div>
    </div>

    <div class="col-sm-4 col-xl-4">
        <div class="p-3 bg-primary-600 rounded overflow-hidden position-relative text-white mb-g">
            <div class="">
                <h3 class="display-4 d-block l-h-n m-0 fw-500">
                    @customerOrderStatistics.VirtualTotalQuantity
                    <small class="m-0 l-h-n">Total Available Quantity</small>
                </h3>
            </div>
            @*<i class="fal fa-location-arrow position-absolute pos-right pos-bottom opacity-15" style="font-size: 6rem;"></i>*@
        </div>
    </div>

    <div class="col-sm-4 col-xl-4">
        <div class="p-3 bg-primary-600 rounded overflow-hidden position-relative text-white mb-g">
            <div class="">
                <h3 class="display-4 d-block l-h-n m-0 fw-500">
                    @customerOrderStatistics.BookedTotalQuantity
                    <small class="m-0 l-h-n">Total Booked Quantity</small>
                </h3>
            </div>
            @*<i class="fal fa-location-arrow position-absolute pos-right pos-bottom opacity-15" style="font-size: 6rem;"></i>*@
        </div>
    </div>

    <div class="col-sm-4 col-xl-4">
        <div class="p-3 bg-primary-600 rounded overflow-hidden position-relative text-white mb-g">
            <div class="">
                <h3 class="display-4 d-block l-h-n m-0 fw-500">
                    @customerOrderStatistics.CustomerConfirmBooking
                    <small class="m-0 l-h-n">Customer Confirm Booking</small>
                </h3>
            </div>
            @*<i class="fal fa-location-arrow position-absolute pos-right pos-bottom opacity-15" style="font-size: 6rem;"></i>*@
        </div>
    </div>

</div>

<div class="row">
    <div class="col-sm-12 col-md-6 col-xl-6">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    Quantity Delivered <span class="fw-300"><i>Last 7 days</i></span>
                </h2>
            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <div class="frame-wrap">
                        <table class="table table-success m-0">
                            <thead>
                                <tr>
                                    <th>Created Date</th>
                                    <th>Total</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (customerOrderStatistics.DeliverdBySevenDayed != null)
                                {
                                    foreach (var item in customerOrderStatistics.DeliverdBySevenDayed)
                                    {
                                        <tr>
                                            <td>@Convert.ToDateTime(item.CreatedDate ?? System.DateTime.Now.ToString()).ToString("dd-MMM-yyyy")</td>
                                            <td>@item.Total</td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-sm-12 col-md-6 col-xl-6">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    Quantity Requested <span class="fw-300"><i>Last 7 days</i></span>
                </h2>
            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <div class="frame-wrap">
                        <table class="table table-success m-0">
                            <thead>
                                <tr>
                                    <th>Created Date</th>
                                    <th>Total</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (customerOrderStatistics.RequestedBySevenDayed != null)
                                {
                                    foreach (var item in customerOrderStatistics.RequestedBySevenDayed)
                                    {
                                        <tr>
                                            <td>@Convert.ToDateTime(item.CreatedDate ?? System.DateTime.Now.ToString()).ToString("dd-MMM-yyyy")</td>
                                            <td>@item.Total</td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

   
<div class="row">
    <div class="col-lg-9 col-md-8">
        <div id="panel-5" class="panel">
            <div class="panel-hdr">
                <h2>Requested Order</h2>
            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <h5>Requested Order History</h5>
                    <div id="flotBar1" style="width: 100%; height: 160px;"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-3 col-md-3">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    News
                </h2>
            </div>
            <div class="panel-container show">
                <div class="panel-content bg-subtlelight-fade">
                    <div id="carouselExampleFade" class="carousel slide carousel-fade" data-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img class="d-block " style="width:100%; height:220px" src="http://awservices.somee.com/ClientDocument/@customerNotificationViewModels[0].ImageUrl" alt="Second slide">
                                <p>
                                    <h4>@customerNotificationViewModels[0].MessageTitle</h4>
                                </p>
                                @customerNotificationViewModels[0].MessageDescription
                            </div>
                            @if (customerNotificationViewModels != null)
                            {
                                for (int i = 1; i < customerNotificationViewModels.Count; i++)
                                {
                                    <div class="carousel-item">
                                        <img class="d-block" style="width:100%; height:220px" src="http://awservices.somee.com/ClientDocument/@customerNotificationViewModels[i].ImageUrl" alt="Second slide">
                                        <p>
                                            <h4>@customerNotificationViewModels[i].MessageTitle</h4>
                                        </p>
                                        @customerNotificationViewModels[i].MessageDescription
                                    </div>
                                }
                            }
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleFade" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleFade" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
   


