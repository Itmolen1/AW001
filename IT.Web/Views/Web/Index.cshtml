@model List<IT.Core.ViewModels.AppVersionViewModel>
@{
    ViewBag.Title = "Aw fuel - App Version";
    Layout = "~/Views/Shared/_layoutAdmin.cshtml";

    List<IT.Core.ViewModels.AppVersionViewModel> appVersionViewModels = new List<IT.Core.ViewModels.AppVersionViewModel>();
    appVersionViewModels = Model as List<IT.Core.ViewModels.AppVersionViewModel>;
}


<div class="row">
    <div class="col-xl-12">
        <div id="panel-1" class="panel">
            <div class="panel-hdr">
                <h2>
                    <a class="btn btn-md btn-primary" href="/Web/AppVersionAdd"> <i class="fal fa-plus-circle"></i>  Create New <span class="fw-300"><i>App version</i></span></a>
                </h2>

            </div>
            <div class="panel-container show">
                <div class="panel-content">
                    <!--      <div class="panel-tag">
                                <p>Drivers Base Information</p>


                            </div>-->
                    <!-- datatable start -->
                    <table id="dt-basic-example" class="table table-bordered table-hover table-striped w-100">
                        <thead class="bg-primary-600">
                            <tr>
                                <th>App Version Name</th>
                                <th>Type</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in appVersionViewModels)
                            {

                                <tr>
                                    <td>@item.AppVersion</td>
                                    <td>@item.AppType</td>
                                    <td>
                                        @* <a href="/Web/Details/@item.Id"><i class="fal fa-eye"></i> </a> &nbsp; &nbsp;*@
                                        <a href="/Web/Index/@item.Id"><i class="fal fa-edit"></i></a>&nbsp; &nbsp;
                                       
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                    <!-- datatable end -->
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts{

    <script>
        $(document).ready(function () {
            $("#MasterMenu").attr("aria-expanded", "true");
            $('#MasterUl').css('display', 'block');
            $('#AppVersionLi').addClass('active');
        });

    </script>

}